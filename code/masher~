full = []
tit = []

fw = open('Mega.bib','w')
new = []

for num in ["0","1","2","3"]:
    fr = open('Cite' + num,'r').readlines()
    for line in fr:
        if line[0] == "@":
            if len(new) > 0:
                full.append(new)
            new = []
            if line not in tit:
                tit.append(line)
        new.append(line)
full = sorted(full,key=str[0].split("{")[1])
for block in full:
    if block[0] in tit:
        tit.remove(block[0])
        for line in block:
            fw.write(line)
